<div class="all-data">
<h3>ALL users</h3>
<h3>Create Users</h3>
<a href="users/new">Click me</a>
<table class="table table-hover">
    <thead>
      <tr>
        <th scope="col">ID</th>
        <th scope="col">Name</th>
        <th scope="col">Show</th>
        <th scope="col">UPDATE</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
  <% @users.each do |i| %>
    <tbody>
      <tr>
        <td><%= i.id %></td>
        <td><%= i.name %></td>
        <td><%= link_to 'show', i %></td>
        <td><%= link_to 'Update',  edit_user_path(i) %></td>
        <td><%= link_to "Delete", user_path(i), method: :delete,data:{ confirm: "Are you sure?"} %></td> 
       
       
        </tr>
    </tbody>
  <% end %>
</table>
